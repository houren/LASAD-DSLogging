<?xml version="1.0" encoding="UTF-8"?>

<ontology type="Allgemeine Argumentation (Gruppennutzung)">
	<elements>
		<element elementid="node" elementtype="box">
			<elementoptions heading="Beitrag" />
			<uisettings width="150" height="150" resizable="true"
				border="round" background-color="#FFD800" font-color="#000000" />
			<childelements>
				<element elementid="content" elementtype="text" quantity="1"	minquantity="1" maxquantity="1">
					<elementoptions />
					<uisettings	minheight="26" />
				</element>
				<element elementid="awareness" elementtype="awareness"
					quantity="1" minquantity="1" maxquantity="1">
					<uisettings minheight="15" maxheight="15" />
				</element>
			</childelements>
		</element>

		<element elementid="undefinedlink" elementtype="relation">
			<elementoptions heading="Undefiniert" endings="false" />
			<uisettings width="133" height="100" resizable="false" linewidth="2px" linecolor="#E6E6E6" background-color="#E6E6E6"/>
			<childelements>
				<element elementid="text" elementtype="text" quantity="0"	minquantity="0" maxquantity="1">
					<elementoptions />
					<uisettings	minheight="26" />
				</element>
				<element elementid="awareness" elementtype="awareness" quantity="1" minquantity="1" maxquantity="1">
					<uisettings	minheight="15" maxheight="15" />
				</element>
			</childelements>
		</element>

		<element elementid="prolink" elementtype="relation" quantity=""
			minquantity="" maxquantity="">
			<elementoptions heading="Pro" endings="true" />
			<uisettings width="112" height="100" resizable="false"
				border="" background-color="#5FC977" font-color="#000000" linewidth="2px"
				linecolor="#5FC977" />
			<childelements>
				<element elementid="text" elementtype="text" quantity="0"
					minquantity="0" maxquantity="1">
					<elementoptions />
					<uisettings background-color="#FFFFFF" font-color="#000000"
						minheight="26" />
				</element>
				<element elementid="awareness" elementtype="awareness"
					quantity="1" minquantity="1" maxquantity="1">
					<uisettings background-color="#FFFFFF" font-color="#000000"
						minheight="15" maxheight="15" />
				</element>
			</childelements>
		</element>

		<element elementid="conlink" elementtype="relation" quantity=""
			minquantity="" maxquantity="">
			<elementoptions heading="Kontra" endings="true" />
			<uisettings width="112" height="100" resizable="false"
				border="" background-color="#FF7979" font-color="#000000" linewidth="2px"
				linecolor="#FF7979" />
			<childelements>
				<element elementid="text" elementtype="text" quantity="0"
					minquantity="0" maxquantity="1">
					<elementoptions />
					<uisettings background-color="#FFFFFF" font-color="#000000"
						minheight="26" />
				</element>
				<element elementid="awareness" elementtype="awareness"
					quantity="1" minquantity="1" maxquantity="1">
					<uisettings background-color="#FFFFFF" font-color="#000000"
						minheight="15" maxheight="15" />
				</element>
			</childelements>
		</element>
	</elements>
</ontology>